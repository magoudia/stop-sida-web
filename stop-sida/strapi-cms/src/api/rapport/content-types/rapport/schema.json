{
  "kind": "collectionType",
  "collectionName": "rapports",
  "info": {
    "singularName": "rapport",
    "pluralName": "rapports",
    "displayName": "rapport"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "titre": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "annee": {
      "type": "biginteger"
    },
    "categorie": {
      "type": "enumeration",
      "enum": [
        "annual",
        "financial",
        "project"
      ]
    },
    "fichier": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "date_publication": {
      "type": "datetime"
    },
    "statut": {
      "type": "enumeration",
      "enum": [
        "brouillon",
        "publie"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "titre"
    },
    "auteur": {
      "type": "string"
    },
    "mots_cles": {
      "type": "text"
    }
  }
}
